{
	"name": "salt/craft-auth0",
	"description": "Authentication with auth0",
	"type": "craft-plugin",
	"version": "2.0.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"craft-auth0"
	],
	"support": {
		"docs": "https://github.com/pangostudio/craft-auth0/blob/master/README.md",
		"issues": "https://github.com/pangostudio/craft-auth0/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Salt",
			"homepage": "https://saltedu.co"
		}
	],
	"require": {
		"craftcms/cms": "^4.0.0",
		"auth0/auth0-php": "^7.9.2"
	},
	"require-dev": {
